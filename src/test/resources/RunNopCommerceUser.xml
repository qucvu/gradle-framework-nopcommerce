<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Run all testcases Nopcommerce - User">
    <parameter name="runConfig" value="local"/>
    <parameter name="osName" value="Windows"/>
    <parameter name="osVersion" value="10"/>
    <parameter name="hubPort" value="4444"/>
    <listeners>
        <listener class-name="reportConfigs.AllureTestListener"/>
    </listeners>
    <test name="Run on Chrome" preserve-order="true">
        <parameter name="browserName" value="h_chrome"/>
        <parameter name="browserVersion" value="125"/>
        <classes>
            <class name="com.nopcommerce.user.User_02_Login"/>
            <!--            <class name="com.nopcommerce.user.User_06_WhistList_Compare"/>-->
        </classes>
    </test>

</suite>